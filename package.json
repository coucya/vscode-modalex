{
    "name": "vscode-modalex",
    "displayName": "ModalEx",
    "description": "A vscode modal editing extension, similar to vim (Including normal, insert, visual, search), and provides vim key presets (todo).",
    "version": "0.3.0",
    "publisher": "couc",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Keymaps",
        "Other"
    ],
    "keywords": [
        "keymaps",
        "modal",
        "vim"
    ],
    "repository": "https://github.com/coucya/vscode-modalex",
    "homepage": "https://github.com/coucya/vscode-modalex",
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "ModalEx",
            "properties": {
                "modalex.preset": {
                    "type": "string",
                    "enum": [
                        "none",
                        "simple"
                    ],
                    "default": "simple",
                    "description": "Preset key mapping."
                },
                "modalex.customKeymaps": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": null,
                    "description": "Customize keymaps file path."
                },
                "modalex.insertTimeout": {
                    "type": "number",
                    "default": 200,
                    "description": "The timeout in milliseconds during insert mode. less than 0 is equivalent to no timeout time."
                },
                "modalex.insertCursorStyle": {
                    "type": "string",
                    "enum": [
                        "block",
                        "block-outline",
                        "line",
                        "line-thin",
                        "underline",
                        "underline-thin"
                    ],
                    "default": "line",
                    "description": "Shape of the cursor when in insert mode."
                },
                "modalex.normalCursorStyle": {
                    "type": "string",
                    "enum": [
                        "block",
                        "block-outline",
                        "line",
                        "line-thin",
                        "underline",
                        "underline-thin"
                    ],
                    "default": "block",
                    "description": "Shape of the cursor when in normal mode."
                },
                "modalex.visualCursorStyle": {
                    "type": "string",
                    "enum": [
                        "block",
                        "block-outline",
                        "line",
                        "line-thin",
                        "underline",
                        "underline-thin"
                    ],
                    "default": "line-thin",
                    "description": "Shape of the cursor when selection is active in normal mode."
                },
                "modalex.keymaps": {
                    "type": "object",
                    "default": {},
                    "properties": {
                        "insert": {
                            "type": "object",
                            "patternProperties": {
                                "id": {
                                    "type": "string",
                                    "description": "id"
                                },
                                "help": {
                                    "type": "string",
                                    "description": "help"
                                },
                                ".": {
                                    "anyOf": [
                                        {
                                            "type": "string",
                                            "description": "VSCode command"
                                        },
                                        {
                                            "type": "object",
                                            "description": "VSCode command with arguments",
                                            "properties": {
                                                "command": {
                                                    "type": "string",
                                                    "description": "VSCode command"
                                                },
                                                "args": {
                                                    "type": "any",
                                                    "description": "Command arguments"
                                                }
                                            }
                                        },
                                        {
                                            "type": "array",
                                            "description": "Sequence of commands",
                                            "items": {
                                                "anyOf": [
                                                    {
                                                        "type": "string",
                                                        "description": "VSCode command"
                                                    },
                                                    {
                                                        "type": "object",
                                                        "description": "VSCode command with arguments",
                                                        "properties": {
                                                            "command": {
                                                                "type": "string",
                                                                "description": "VSCode command"
                                                            },
                                                            "args": {
                                                                "type": "any",
                                                                "description": "Command arguments"
                                                            }
                                                        }
                                                    }
                                                ]
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "description": "keymap"
                                        }
                                    ]
                                }
                            }
                        },
                        "normal": {
                            "type": "object",
                            "patternProperties": {
                                "id": {
                                    "type": "string",
                                    "description": "id"
                                },
                                "help": {
                                    "type": "string",
                                    "description": "help"
                                },
                                ".": {
                                    "anyOf": [
                                        {
                                            "type": "string",
                                            "description": "VSCode command"
                                        },
                                        {
                                            "type": "object",
                                            "description": "VSCode command with arguments",
                                            "properties": {
                                                "command": {
                                                    "type": "string",
                                                    "description": "VSCode command"
                                                },
                                                "args": {
                                                    "type": "any",
                                                    "description": "Command arguments"
                                                }
                                            }
                                        },
                                        {
                                            "type": "array",
                                            "description": "Sequence of commands",
                                            "items": {
                                                "anyOf": [
                                                    {
                                                        "type": "string",
                                                        "description": "VSCode command"
                                                    },
                                                    {
                                                        "type": "object",
                                                        "description": "VSCode command with arguments",
                                                        "properties": {
                                                            "command": {
                                                                "type": "string",
                                                                "description": "VSCode command"
                                                            },
                                                            "args": {
                                                                "type": "any",
                                                                "description": "Command arguments"
                                                            }
                                                        }
                                                    }
                                                ]
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "description": "keymap"
                                        }
                                    ]
                                }
                            }
                        },
                        "visual": {
                            "type": "object",
                            "patternProperties": {
                                "id": {
                                    "type": "string",
                                    "description": "id"
                                },
                                "help": {
                                    "type": "string",
                                    "description": "help"
                                },
                                ".": {
                                    "anyOf": [
                                        {
                                            "type": "string",
                                            "description": "VSCode command"
                                        },
                                        {
                                            "type": "object",
                                            "description": "VSCode command with arguments",
                                            "properties": {
                                                "command": {
                                                    "type": "string",
                                                    "description": "VSCode command"
                                                },
                                                "args": {
                                                    "type": "any",
                                                    "description": "Command arguments"
                                                }
                                            }
                                        },
                                        {
                                            "type": "array",
                                            "description": "Sequence of commands",
                                            "items": {
                                                "anyOf": [
                                                    {
                                                        "type": "string",
                                                        "description": "VSCode command"
                                                    },
                                                    {
                                                        "type": "object",
                                                        "description": "VSCode command with arguments",
                                                        "properties": {
                                                            "command": {
                                                                "type": "string",
                                                                "description": "VSCode command"
                                                            },
                                                            "args": {
                                                                "type": "any",
                                                                "description": "Command arguments"
                                                            }
                                                        }
                                                    }
                                                ]
                                            }
                                        },
                                        {
                                            "type": "object",
                                            "description": "keymap"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "commands": [
            {
                "command": "modalex.enable",
                "title": "Enable ModalEx"
            },
            {
                "command": "modalex.disable",
                "title": "Disable ModalEx"
            },
            {
                "command": "modalex.reload",
                "title": "ModalEx reload config"
            },
            {
                "command": "modalex.editCustomKeymaps",
                "title": "ModalEx edit custom keymaps"
            }
        ],
        "keybindings": [
            {
                "key": "Escape",
                "command": "modalex.enterNormal",
                "when": "editorTextFocus && modalex.isEnable"
            }
        ]
    },
    "scripts": {
        "package": "tsc & vite build & vsce package --no-dependencies",
        "publish": "vsce publish --no-dependencies",
        "build": "tsc & vite build"
    },
    "devDependencies": {
        "@types/node": "^20.14.15",
        "@types/vscode": "^1.80.0",
        "typescript": "^5.5.4",
        "vite": "^5.4.0"
    }
}